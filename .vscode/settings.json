  "i18n-ally.localesPaths": ["src/i18n"],
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "flat",
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{locale}.json",

  "prettier.useEditorConfig": false,
  "prettier.useTabs": false,
  "prettier.configPath": ".prettierrc",

  "notebook.formatOnSave.enabled": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.extract.ignoredByFiles": {
    "src\\layouts\\MediaPlayerLayout.vue": ["Media Player Window"]
  },
  "i18n-ally.extract.ignored": ["Meeting Media Manager"],
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "i18n-ally.keysInUse": [
    "mediaRetrievalAndPlaybackDescription",
    "applicationConfigurationDescription",
    "congregationMeetingsDescription",
    "mediaRetrievalAndPlayback",
    "applicationConfiguration",
    "enableMediaDisplayButton",
