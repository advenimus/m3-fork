    "nofile",
    "nsis",
    "objt",
    "overscan",
    "Pangasinan",
    "panzoom",
    "Panzoom",
    "panzoomend",
    "panzooms",
    "persistedstate",
    "Persistedstate",
    "pjpeg",
    "Português",
    "prefs",
    "Savignac",
    "Scrp",
    "sircharlo",
    "sjjm",
    "Slovenčina",
    "Slovenščina",
    "snnw",
    "sqlite",
    "Svenska",
    "svgs",
    "themify",
    "unregistering",
    "upath",
    "Uuids",
    "vitepress",
    "wtlocale",
